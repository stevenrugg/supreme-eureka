generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model GuestbookEntry {
  id        Int      @id @default(autoincrement())
  name      String?
  message   String
  createdAt DateTime @default(now())
}
